{
  "manifest_version": 3,
  "name": "LexoFin Extension",
  "description": "Get a summary of news articles using the chatGpt api",
  "version": "1.0",
  "permissions": ["activeTab", "https://*/*", "http://*/*", "storage"],
  "content_security_policy": "script-src 'self' https://api.openai.com; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html"
  }
}
